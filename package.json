{
  "name": "movie-draft",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "watch": "concurrently --name 'webpack, stylus, api' --prefix name 'npm run start' 'npm run styles:watch' 'npm run api-server-dev'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "api-server": "node ./src/api/server.js",
    "api-server-dev": "nodemon --inspect ./src/api/server.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "history": "4.2.0",
    "mongodb": "^3.0.4",
    "re-base": "2.2.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "autoprefixer-stylus": "0.10.0",
    "concurrently": "3.0.0",
    "nodemon": "^1.17.1",
    "stylus": "0.54.5"
  }
}
