.app
  text-align: center
  .app-header
    background-color: #222
    height: 150px
    padding: 20px
    color: white
  .app-logo
    animation: rotate infinite 20s linear
    height: 80px
  .app-title
    font-size: large

@keyframes rotate
  from
    transform: rotate(0deg)
  to
    transform: rotate(360deg)